<?xml version="1.0" encoding="utf-8" ?>
<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <SelectImageFromFotoWareDialog>
    <FormDialog Header="Bild aus FotoWare auswählen" Text="Bitte wählen Sie ein Bild aus FotoWare aus" OKButton="OK">
      <CodeBeside Type="Kingmaddi.Foundation.FotoWareExtension.Dialogs.SelectImageFromFotoWareDialog, Kingmaddi.Foundation.FotoWareExtension" />

      <Script Src="/sitecore modules/Shell/FotoWareExtension/js/jquery-3.5.1.min.js" Type="text/javascript" />
      
      <Edit ID="RawValue" Class="" Hidden="True" runat="server" />
      <Edit ID="AccessToken" Class="" Hidden="True" runat="server" />
      <Edit ID="TenantUrl" Class="" Hidden="True" runat="server" />

      <Border Padding="0 0 5 0" Height="100%">
        <div class="container control">
          <div id="fotowarecontainer" class="container">
            <h1 id="errorMessage" style="display: none;">
              Fehler bei der Authentifizierung aufgetreten.
            </h1>
          </div>
        </div>
      </Border>
    
      <Script Src="/sitecore modules/Shell/FotoWareExtension/js/select-image.js" Type="text/javascript" />
      <Script Src="/sitecore modules/Shell/FotoWareExtension/js/select-image-rich-text-editor.js" Type="text/javascript" />
    </FormDialog>
  </SelectImageFromFotoWareDialog>
</control>